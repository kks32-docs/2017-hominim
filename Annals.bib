@Article{Tarrago2003,
  Title                    = {{Different cytomegalovirus glycoprotein B genotype distribution in serum and cerebrospinal fluid specimens determined by a novel multiplex nested {PC}R.}},
  Author                   = {Tarrag{\'{o}}, David and Quereda, Carmen and Tenorio, Antonio},
  Journal                  = {Journal of clinical microbiology},
