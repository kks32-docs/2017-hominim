@Article{Ye2017,
  Title                    = {{Adaptation of the FADS gene family in {Europe}: Variation across time, geography and subsistence}},
  Author                   = {Ye, Kaixiong and Gao, Feng and Wang, David and Bar-Yosef, Ofer and Keinan, Alon},
  Journal                  = {bioRxiv},
  Year                     = {2017},
  Pages                    = {111229},

  Abstract                 = {The FADS gene family encodes rate-limiting enzymes for the biosynthesis of omega-6 and omega-3 long chain polyunsaturated fatty acids (LCPUFAs), which is essential for individuals subsisting on LCPUFAs-poor diets (e.g. plant-based). Positive selection on FADS genes has been reported in multiple populations, but its presence and pattern in Europeans remain elusive. Here, with analyses of ancient and modern DNA, we demonstrated positive selection acted on variants centered on FADS1 and FADS2 both before and after the advent of farming in Europe, but adaptive alleles in these two periods are opposite. Selection signals in recent history also vary geographically, with the strongest in Southern Europe. We showed that adaptive alleles in recent farmers are associated with expression of FADS genes, enhanced LCPUFAs biosynthesis and reduced risk of inflammatory bowel diseases. Thus, the adaptation of FADS genes in Europe varies across time and geography, probably due to varying diet and subsistence.},
  Doi                      = {10.1101/111229},
  File                     = {:C$\backslash$:/Users/ch504/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ye et al. - 2017 - Adaptation of the FADS gene family in Europe Variation across time, geography and subsistence.pdf:pdf},
  Publisher                = {Cold Spring Harbor Labs Journals},
  Url                      = {http://biorxiv.org/content/early/2017/02/23/111229}
}

@Article{Yeh2016,
  Title                    = {{Early evidence for travel with infectious diseases along the Silk Road: Intestinal parasites from 2000 year-old personal hygiene sticks in a latrine at Xuanquanzhi Relay Station in China}},
  Author                   = {Yeh, Hui-Yuan and Mao, Ruilin and Wang, Hui and Qi, Wuyun and Mitchell, Piers D.},
  Journal                  = {Journal of Archaeological Science: Reports},
  Year                     = {2016},
  Pages                    = {758--764},
  Volume                   = {9},

  Abstract                 = {The Silk Road has often been blamed for the spread of infectious diseases in the past between East Asia, the Middle East and Europe. While such a hypothesis seems plausible, there is actually very little concrete evidence to prove that diseases were transmitted by early travellers moving along its various branches. The aim of this study is to look for ancient parasite eggs on personal hygiene sticks in a latrine at a large relay station on the Silk Road at Xuanquanzhi (111 BCE--CE 109), at the eastern margin of the Tarim Basin in north-western China. We isolated eggs of four species of parasitic intestinal worms: Chinese liver fluke (Clonorchis sinensis), Taenia sp. tapeworm (likely Taenia asiatica, Taenia solium or Taenia saginata), roundworm (Ascaris lumbricoides) and whipworm (Trichuris trichiura). The Chinese liver fluke requires wet marshy areas to sustain its life cycle and could not have been endemic to this arid region. The presence of this species suggests that people from well-watered areas of eastern or southern China travelled with their parasites to this relay station along the Silk Road, either for trade or on government business. This appears to be the earliest archaeological evidence for travel with infectious diseases along the Silk Road.},
  Doi                      = {10.1016/j.jasrep.2016.05.010},
  File                     = {:C$\backslash$:/Users/ch504/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Yeh et al. - 2016 - Early evidence for travel with infectious diseases along the Silk Road Intestinal parasites from 2000 year-old perso.pdf:pdf;:C$\backslash$:/Users/ch504/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Yeh et al. - 2016 - Early evidence for travel with infectious diseases along the Silk Road Intestinal parasites from 2000 year-old pe(2).pdf:pdf},
