@Article{Choy2016,
  Title                    = {{Chemical profiling of ancient hearths reveals recurrent salmon use in Ice Age Beringia}},
  Author                   = {Choy, Kyungcheol and Potter, Ben A and McKinney, Holly J. and Reuther, Joshua D and Wang, Shiway W and Wooller, Matthew J},
  Journal                  = {Proceedings of the National Academy of Sciences},
  Year                     = {2016},
  Number                   = {35},
  Pages                    = {9757--9762},
  Volume                   = {113},

  Abstract                 = {Current approaches to reconstruct subsistence and dietary trends in ancient hunter-gatherer societies include stable isotope analyses, but these have focused on human remains, cooking pottery, and food residues, which are relatively rare in the archaeological record. In contrast, short-term hearths are more ubiquitous worldwide, and these features can provide valuable evidence for ancient subsistence practices, particularly when faunal remains are not preserved. To test the suitability of hearths for this purpose, we conducted multiple chemical analyses: stable carbon and nitrogen isotope analyses of total organic matter (expressed as $\delta$13C and $\delta$15N values) and compound-specific carbon isotope analyses of individual fatty acids ($\delta$13C16:0 and $\delta$13C18:0) from 17 well-preserved hearths present in three occupations dating between $\sim$13,200--11,500 calibrated years B.P. at the Upward Sun River (USR) site in central Alaska. We combined $\delta$15N and $\delta$13CFA data in a Bayesian mixing model (stable isotope analysis in R) with concentration dependency to each hearth. Our model values were tested against faunal indices, indicating a strong positive relationship between marine proportional contributions to each hearth and salmon abundance. Results of the models show substantial anadromous salmon use in multiple USR components, indicating recurrent use of the site for salmon processing during the terminal Pleistocene. Our results demonstrate that salmonid and freshwater resources were more important for late Pleistocene hunter-gatherers than previously thought and highlight the potential of chemical profiling of hearth organic residues for providing greater geographic and temporal insights into resource use by prepottery societies.},
  Doi                      = {10.1073/pnas.1606219113},
  File                     = {:C$\backslash$:/Users/ch504/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Choy et al. - Unknown - Chemical profiling of ancient hearths reveals recurrent salmon use in Ice Age Beringia.pdf:pdf},
  ISBN                     = {0711232105},
  ISSN                     = {0027-8424, 1091-6490},
  Url                      = {http://www.pnas.org/content/113/35/9757.full.pdf http://www.pnas.org/content/113/35/9757{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/27573838{\%}5Cnhttp://www.pnas.org/content/113/35/9757.abstract}
}

