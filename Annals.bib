@Article{Gr??n2005,
  Title                    = {{U-series and ESR analyses of bones and teeth relating to the human burials from Skhul.}},
  Author                   = {Gr{\"{u}}n, Rainer and Stringer, Chris and McDermott, Frank and Nathan, Roger and Porat, Naomi and Robertson, Steve and Taylor, Lois and Mortimer, Graham and Eggins, Stephen and McCulloch, Malcolm},
