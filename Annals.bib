@Article{Chattopadhyay2015,
  Title                    = {{The combined risks of reduced or increased function variants in cell death pathway genes differentially influence cervical cancer risk and herpes simplex virus type 2 infection among black {Africa}ns and the Mixed Ancestry population of South Africa}},
  Author                   = {Chattopadhyay, Koushik and Williamson, Anna-Lise and Hazra, Annapurna and Dandara, Collet},
  Journal                  = {BMC Cancer},
  Year                     = {2015},

  Month                    = dec,
  Number                   = {1},
  Pages                    = {680},
  Volume                   = {15},

  Abstract                 = {BACKGROUND Cervical cancer is one of the most important cancers worldwide with a high incident and mortality rate and is caused by the human papilloma virus (HPV). Among sexually active women who get infected with human papillomavirus (HPV), a small fraction progresses to cervical cancer disease pointing to possible roles of additional risk factors in development of the disease which include host genetic factors and other infections such as HSV-2. Since cellular apoptosis plays a role in controlling the spread of virus-infections in cells, gene variants altering the function of proteins involved in cell death pathways might be associated with the clearing of virus infections. Activity altering polymorphisms in FasR (-1377G\hspace{0.167em}{\textgreater}\hspace{0.167em}A and -670A\hspace{0.167em}{\textgreater}\hspace{0.167em}G), FasL (-844 T\hspace{0.167em}{\textgreater}\hspace{0.167em}C) and CASP8 (-652 6 N ins/del) genes have been shown to alter the mechanism of apoptosis by modifying the level of expression of their correspondent proteins. In the present study, we set out to investigate the combined risks of CASP8, FasR, and FasL polymorphisms in cervical cancer, pre-cancerous lesions, HPV infection and HSV-2 infection. METHODS Participants were 442 South African women of black African and mixed-ancestry origin with invasive cervical cancer and 278 control women matched by age, ethnicity and domicile status. FasR and FasL polymorphisms were genotyped by TaqMan and CASP8 polymorphism by PCR-RFLP. The results were analysed with R using haplo.stats software version 1.5.2. RESULTS CASP8 -652 6 N del\hspace{0.167em}+\hspace{0.167em}FasR-670A was associated with a reduced risk (P\hspace{0.167em}=\hspace{0.167em}0.019, Combined Polymorphism Score (CPS)\hspace{0.167em}=\hspace{0.167em}-2.34) and CASP8 -652 6 N ins\hspace{0.167em}+\hspace{0.167em}FasR-1377G was associated with a marginal increased risk (P\hspace{0.167em}=\hspace{0.167em}0.047, CPS\hspace{0.167em}=\hspace{0.167em}1.99) of cervical cancer among black Africans. When compared within the control group, CASP8 -652 6 N ins\hspace{0.167em}+\hspace{0.167em}FasR-1377A showed a reduced risk (P\hspace{0.167em}=\hspace{0.167em}0.023, CPS\hspace{0.167em}=\hspace{0.167em}-2.28) of HSV-2 infection in both black African and mixed-ancestry population. CONCLUSIONS Our results show that the combined risks of variants in cell death pathway genes are associated with the cervical cancer as well as the HSV-2 infection in the black African and mixed-ancestry population.},
  Doi                      = {10.1186/s12885-015-1678-y},
  ISSN                     = {1471-2407},
  Pmid                     = {26458812},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/26458812 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4603903}
}

