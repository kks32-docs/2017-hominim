@Article{Cloutman-Green2010,
  Title                    = {{Effectiveness and efficiency of current cleaning protocols on Adenovirus in the environment-5 years data analysis in a UK children's hospital}},
  Author                   = {Cloutman-Green, E and Hartley, J},
  Journal                  = {Journal of Hospital Infection},
  Year                     = {2010},
  Pages                    = {S57--S58},
  Volume                   = {76},

  Abstract                 = {Background: Adenovirus infection is common, causing between 2 and 8{\%} of childhood respiratory infections, with most primary infection occurring between the ages of 6 months and 5 years. Children with underlying diseases who undergo prolonged inpatient stays are at high risk of severe Adenovirus infection and represent a source for nosocomial transmission. Acquisition or reactivation of Adenovirus during hematopoieitic stem cell transplantation (HSCT) can lead to high morbidity and mortality in this patient group. Immunosuppressed patients tend to demonstrate prolonged high levels of viral excretion. Aims: Surveillance of the environment to inform strategies to prevent Adenovirus cross transmission and to monitor response to cleaning. Methods: In the HSCT unit environmental screening occurs in cubicles that have been occupied by an Adenovirus positive patient, after the post discharge 'deep clean'. 10-12 sites are sampled including the floor and susceptible hard to clean and high touch areas. Sites are screened using a molecular real time PCR assay. Results: Screening occupied cubicles before 'deep cleaning' occurred, has demonstrated that significant levels of contamination exist within these bed spaces. Importantly the exit door handle from the cubicles were always contaminated. This contamination represents a significant risk, as there is potential for the virus to be carried outside of the cubicle, even when staff and visitors adhere to current infection control guidance. Screening has shown that even if a room passes a visual inspection, post 'deep clean' organisms can still be detected within the environment that can lead to health care associated infection. It is felt that cleaning only removes between 95-99{\%} of the viral load within the cubicles and many cubicles are very highly contaminated at discharge, with few cubicles passing environmental screening with only 1 clean. Cubicles on average require 2-5 'deep cleans' before reaching 'clean' levels.},
  ISBN                     = {0195-6701},
  Keywords                 = {Adenoviridae,United Kingdom,adenovirus infection,assay,child,childhood,contamination,data analysis,environment,excretion,health care,hospital infection,hospital patient,infection,infection control,morbidity,mortality,patient,pediatric hospital,primary infection,real time polymerase chain reaction,respiratory tract infection,risk,screening,society,stem cell transplantation,virus,virus load},
  Url                      = {http://www.embase.com/search/results?subaction=viewrecord{\&}from=export{\&}id=L70386754{\%}5Cnhttp://ucelinks.cdlib.org:8888/sfx{\_}ucsf?sid=EMBASE{\&}issn=01956701{\&}id=doi:{\&}atitle=Effectiveness+and+efficiency+of+current+cleaning+protocols+on+Adenovirus+in+the+environme}
}

@Article{Coble2011,
  Title                    = {{The identification of the Romanovs: Can we (finally) put the controversies to rest?}},
  Author                   = {Coble, Michael D},
  Journal                  = {Investigative genetics},
  Year                     = {2011},

  Month                    = sep,
  Number                   = {1},
  Pages                    = {20},
  Volume                   = {2},

  Abstract                 = {For much of the 20th century the fate of the last Imperial family of Russia, the Romanovs, was a mystery after their execution in 1918. In the mid 1970s the mass grave of the Romanov family (minus two of the children) was discovered and officially exhumed after the fall of the Soviet Union. Forensic DNA testing of the remains in the early 1990s was used to identify the family. Despite the overwhelming evidence for establishing the identity of the Romanov family, a small but vocal number of scientists have tried to raise doubt about the DNA testing during the late 1990s and early 2000s. With the discovery of the two missing Romanov children in 2007, there was an opportunity to re-analyze all of the evidence associated with the case which confirmed the initial DNA testing and brought finality to the mystery. This article will discuss the controversies associated with the Romanov identification and reflect upon the importance of the case to the field of forensic DNA typing over the last 20 years.},
  Doi                      = {10.1186/2041-2223-2-20},
  File                     = {:C$\backslash$:/Users/ch504/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Coble - 2011 - The identification of the Romanovs Can we (finally) put the controversies to rest.pdf:pdf},
  ISSN                     = {2041-2223},
  Pmid                     = {21943354},
  Publisher                = {BioMed Central},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/21943354 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3205009}
}

