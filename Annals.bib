  Arxivid                  = {arXiv:1011.1669v3},
  Booktitle                = {CRC},
  Doi                      = {doi:10.1201/b10391-4},
  Eprint                   = {arXiv:1011.1669v3},
  ISBN                     = {1-58488-387-1},
  ISSN                     = {14337851},
  Keywords                 = {KorbAnn2003},
  Pages                    = {29--54},
  Pmid                     = {50816598}
}

@Article{KosakovskyPond2006,
