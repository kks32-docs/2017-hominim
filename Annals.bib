@Article{Brown2015,
  Title                    = {{Rapid whole-genome sequencing of mycobacterium tuberculosis isolates directly from clinical samples}},
  Author                   = {Brown, Amanda C. and Bryant, Josephine M. and Einer-Jensen, Katja and Holdstock, Jolyon and Houniet, Darren T. and Chan, Jacqueline Z M and Depledge, Daniel P. and Nikolayevskyy, Vladyslav and Broda, Agnieszka and Stone, Madeline J. and Christiansen, Mette T. and Williams, Rachel and McAndrew, Michael B. and Tutill, Helena and Brown, Julianne and Melzer, Mark and Rosmarin, Caryn and McHugh, Timothy D. and Shorten, Robert J. and Drobniewski, Francis and Speight, Graham and Breuer, Judith},
  Journal                  = {Journal of Clinical Microbiology},
  Year                     = {2015},
  Number                   = {7},
  Pages                    = {2230--2237},
  Volume                   = {53},

  Abstract                 = {{\textless}p{\textgreater} The rapid identification of antimicrobial resistance is essential for effective treatment of highly resistant {\textless}named-content content-type="genus-species"{\textgreater}Mycobacterium tuberculosis{\textless}/named-content{\textgreater} . Whole-genome sequencing provides comprehensive data on resistance mutations and strain typing for monitoring transmission, but unlike for conventional molecular tests, this has previously been achievable only from cultures of {\textless}named-content content-type="genus-species"{\textgreater}M. tuberculosis{\textless}/named-content{\textgreater} . Here we describe a method utilizing biotinylated RNA baits designed specifically for {\textless}named-content content-type="genus-species"{\textgreater}M. tuberculosis{\textless}/named-content{\textgreater} DNA to capture full {\textless}named-content content-type="genus-species"{\textgreater}M. tuberculosis{\textless}/named-content{\textgreater} genomes directly from infected sputum samples, allowing whole-genome sequencing without the requirement of culture. This was carried out on 24 smear-positive sputum samples, collected from the United Kingdom and Lithuania where a matched culture sample was available, and 2 samples that had failed to grow in culture. {\textless}named-content content-type="genus-species"{\textgreater}M. tuberculosis{\textless}/named-content{\textgreater} sequencing data were obtained directly from all 24 smear-positive culture-positive sputa, of which 20 were of high quality ({\textgreater}20$\times$ depth and {\textgreater}90{\%} of the genome covered). Results were compared with those of conventional molecular and culture-based methods, and high levels of concordance between phenotypical resistance and predicted resistance based on genotype were observed. High-quality sequence data were obtained from one smear-positive culture-negative case. This study demonstrated for the first time the successful and accurate sequencing of {\textless}named-content content-type="genus-species"{\textgreater}M. tuberculosis{\textless}/named-content{\textgreater} genomes directly from uncultured sputa. Identification of known resistance mutations within a week of sample receipt offers the prospect for personalized rather than empirical treatment of drug-resistant tuberculosis, including the use of antimicrobial-sparing regimens, leading to improved outcomes. {\textless}/p{\textgreater}},
  Doi                      = {10.1128/JCM.00486-15},
  ISBN                     = {1098-660X (Electronic)$\backslash$r0095-1137 (Linking)},
  ISSN                     = {1098660X},
  Pmid                     = {25972414}
}

