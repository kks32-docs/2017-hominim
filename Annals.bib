  Keywords                 = {ESR dating,Early modern humans,Skhul,Skhul II,Skhul IX,Skhul V,U-series dating},
  Pmid                     = {15970310},
  Url                      = {http://linkinghub.elsevier.com/retrieve/pii/S0047248405000679 http://www.ncbi.nlm.nih.gov/pubmed/15970310}
}

