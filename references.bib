keywords = {Borrelia burgdorferi,Borrelia burgdorferi: classification,Borrelia burgdorferi: genetics,DNA, Bacterial,DNA, Bacterial: metabolism,Europe,Genes, Bacterial,Genetic Variation,Genome, Bacterial,Lyme Disease,Lyme Disease: epidemiology,Lyme Disease: genetics,Microbiology,Molecular Sequence Data,North America,Phylogeny,Sequence Analysis, DNA,infectious disease,paleopathology},
