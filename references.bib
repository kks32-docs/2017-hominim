journal = {Genome research},
month = {mar},
number = {3},
pages = {291--300},
pmid = {26888264},
publisher = {Cold Spring Harbor Laboratory Press},
title = {{Model-based analyses of whole-genome data reveal a complex evolutionary history involving archaic introgression in Central African Pygmies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26888264 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4772012},
volume = {26},
