journal = {Science},
keywords = {African Continental Ancestry Group,African Continental Ancestry Group: genetics,Asia,Biological Evolution,Ethiopia,Europe,Genetic Variation,Genome,Human,Human Migration,Humans,Male},
number = {6262},
pages = {820--2},
