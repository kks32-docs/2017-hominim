journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Ancient,Archaeology,Carbohydrates,Carbohydrates: chemistry,Dental Caries,Dental Caries: epidemiology,Dental Caries: history,Dental Caries: microbiology,Dietary Sucrose,Edible,Food Habits,Food Supply,Geography,History,Humans,Mass Spectrometry,Morocco,Paleodontology,Plants,Prevalence,Streptococcus mutans,paleopathology},
