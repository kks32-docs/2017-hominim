doi = {10.1073/pnas.0914088107},
isbn = {0027-8424},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Anthropology,Coloring Agents,Minerals,Mollusca,Spain},
number = {3},
pages = {1023--8},
pmid = {20080653},
title = {{Symbolic use of marine shells and mineral pigments by Iberian Neandertals.}},
