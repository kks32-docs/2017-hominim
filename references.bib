@article{Chan2004,
abstract = {BACKGROUND The discovery of fetal DNA in maternal plasma has opened up an approach for noninvasive prenatal diagnosis. Despite the rapid expansion in clinical applications, the molecular characteristics of plasma DNA in pregnant women remain unclear. METHODS We investigated the size distribution of plasma DNA in 34 nonpregnant women and 31 pregnant women, using a panel of quantitative PCR assays with different amplicon sizes targeting the leptin gene. We also determined the size distribution of fetal DNA in maternal plasma by targeting the SRY gene. RESULTS The median percentages of plasma DNA with size {\textgreater}201 bp were 57{\%} and 14{\%} for pregnant and nonpregnant women, respectively (P {\textless}0.001, Mann-Whitney test). The median percentages of fetal-derived DNA with sizes {\textgreater}193 bp and {\textgreater}313 bp were 20{\%} and 0{\%}, respectively, in maternal plasma. CONCLUSION Plasma DNA molecules are mainly short DNA fragments. The DNA fragments in the plasma of pregnant women are significantly longer than those in the plasma of nonpregnant women, and the maternal-derived DNA molecules are longer than the fetal-derived ones.},
author = {Chan, K C Allen and Zhang, Jun and Hui, Angela B Y and Wong, Nathalie and Lau, Tze K and Leung, Tse N and Lo, Kwok-Wai and Huang, Dolly W S and Lo, Y M Dennis},
doi = {10.1373/clinchem.2003.024893},
issn = {0009-9147},
journal = {Clinical chemistry},
