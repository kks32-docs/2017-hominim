issn = {0002-9297},
journal = {American journal of human genetics},
keywords = {Base Sequence,Caspase 12,Caspases,Caspases: genetics,Caspases: metabolism,Codon, Terminator,DNA Primers,Haplotypes,Humans,Mesolithic,Molecular Sequence Data,Mutation,Paleopathology,Phylogeny,Polymorphism, Genetic,Selection, Genetic,genetics},
language = {en},
