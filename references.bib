issn = {0047-2484},
journal = {Journal of human evolution},
keywords = {Animals,Archaeology,Biological Evolution,Fossils,History, Ancient,Hominidae,Humans,Occupations,Occupations: history,Paleodontology,Paleontology,Radiometric Dating,Turkey},
mendeley-tags = {Archaeology},
